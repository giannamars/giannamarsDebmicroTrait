{
    "ver": "0.0.1",
    "authors": [
        "giannamars"
    ],
    "contact": "",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": null
    },
    "parameter-groups": [
        {
            "id": "trait_selection",
            "parameters": [
                "length",
                "completeness",
                "contamination"
            ],
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "with_border": true
        }
    ],
    "parameters": [ 
        {
            "id": "media_refs",
            "optional": true,
            "advanced": false,
            "allow_multiple": true,
            "default_values": [
                ""
            ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [
                    "KBaseBiochem.Media",
                    "KBaseMatrices.ChemicalAbundanceMatrix"
                ]
            }
        },
        {
            "id": "length",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [
                "50000"
            ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_int": "0"
            }
        },
        {
            "id": "completeness",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [
                "75"
            ],
            "field_type": "text",
            "text_options": {
                "validate_as": "float",
                "min_float": "0",
                "max_float": "100"
            }
        },
        {
            "id": "contamination",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [
                "25"
            ],
            "field_type": "text",
            "text_options": {
                "validate_as": "float",
                "min_float": "0",
                "max_float": "100"
            }
        },
        {
            "id": "simulation_type",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [
                "batch"
            ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": "batch",
                        "display": "Batch Mode",
                        "id": "batch",
                        "ui-name": "batch - default simulation type"
                    },
                    {
                        "value": "mixed_media",
                        "display": "Mixed Mode",
                        "id": "mixed_media",
                        "ui-name": "mixed_media - mixed media"
                    }
                    ,
                    {
                        "value": "competition",
                        "display": "Competition",
                        "id": "competition",
                        "ui-name": "competition - competition"
                    },
                    {
                        "value": "fba_media",
                        "display": "KBase Media",
                        "id": "fba_media",
                        "ui-name": "fba_media - fba_media"
                    }
                ]
            }
        },
        {
            "id": "resource_use",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "1" ],
            "field_type": "checkbox",
            "checkbox_options": {
                "unchecked_value": 0,
                "checked_value": 1
            }
        },
        {
            "id": "batch_concentration",
            "optional": false,
            "advanced": false,
            "allow_multiple": true,
            "default_values": [ "1.25" ],
            "field_type": "text"
        }

    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "giannamarsDebmicroTrait",
            "method": "run_giannamarsDebmicroTrait",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "resource_use",
                    "target_property": "resource_use"
                },
                {
                    "input_parameter": "media_refs",
                    "target_property": "media_refs",
                    "target_type_transform": "list<ref>"
                },{
                    "input_parameter": "simulation_type",
                    "target_property": "simulation_type"
                },{
                    "input_parameter": "batch_concentration",
                    "target_property": "batch_concentration"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },{
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
